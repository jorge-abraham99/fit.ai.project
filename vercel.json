{
    "version": 2,
    "builds": [
      {
        "src": "package.json",     
        "use": "@vercel/next"
      },
      {
        "src": "api-python/**/*.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/meal_plan",
        "dest": "/api-python/meal_plan.py"
      },
  
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/" }
    ]
  }